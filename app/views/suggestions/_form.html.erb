<%= form_for @suggestion do |f| %>
  <%= render "shared/error_messages", object: @suggestion %>
  <div class="form-group">
    <%= f.label :name, t("form.name") %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :describe, t("label.info") %>
    <%= f.text_area :describe, class: "form-control" %>
  </div>
  </br>
  <%= f.submit yield(:button_text), class: "form-control btn-info" %>
<% end %>
